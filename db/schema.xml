<?xml version="1.0" encoding="UTF-8" ?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/propelorm/Propel2/master/resources/xsd/database.xsd"
		  name="spajam" namespace="ORM">
	<table name="user">
		<column name="user_id" type="integer" primaryKey="true" autoIncrement="true"/>
		<column name="token" type="varchar" size="255"/>
		<unique>
			<unique-column name="token"/>
		</unique>
	</table>
	<table name="shop">
		<column name="shop_id" type="integer" primaryKey="true" autoIncrement="true"/>
		<column name="name" type="varchar" size="255"/>
	</table>
</database>
